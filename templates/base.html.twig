<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Instakilo
			{% endblock %}
		</title>

		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<!-- CSS only -->
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@5.1.3/dist/cosmo/bootstrap.min.css">
			<link
			rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootswatch@5.1.3/dist/cosmo/_variables.scss">
		{# 
		   
		    <link rel="stylesheet" href="style/main.css"> #}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body></body>
	<body>
		{% block body %}
				{% block head %}
					{% block nav %}
						{% include "header/navigation.html.twig" %}
					{% endblock %}
				{% endblock %}

			{% block content %}{% endblock %}

			{% block footer %}
				{% include "footer/footer.html.twig" %}
			{% endblock %}

		{% endblock %}
	</body>
</html></body></html>
